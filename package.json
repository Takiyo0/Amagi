{
  "name": "amagi",
  "version": "1.1.1",
  "description": "A lavalink track search module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "test/index.js",
    "build:full": "npm run lint && npm run format && npm run build:docs && npm run build",
    "build": "tsc",
    "format": "prettier --write \"src/**/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build:docs": "typedoc --readme README.md --out docs/ --entryPointStrategy expand src/."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Takiyo0/Amagi.git"
  },
  "keywords": [
    "lavalink",
    "lavalink-search",
    "lavalink-track",
    "lavalink-searcher",
    "lavalink-rate-limit",
    "discord"
  ],
  "author": "Takiyo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Takiyo0/Amagi/issues"
  },
  "homepage": "https://github.com/Takiyo0/Amagi#readme",
  "dependencies": {
    "flat-cache": "^3.0.4",
    "undici": "^5.5.1"
  },
  "devDependencies": {
    "@types/flat-cache": "^2.0.0",
    "@types/node": "^18.0.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.1",
    "typescript": "^4.7.4"
  }
}